
application {
  config {
    baseName myApp,
    applicationType gateway,
    packageName com.myapp,
    authenticationType jwt,
    prodDatabaseType mysql,
    clientFramework angular
  }
  entities *
}

entity Location {
	streetAddress String,
	postalCode String,
	city String,
	stateProvince String
}
entity Customer {
	Name String
}
 relationship OneToMany {
  Location to Customer
}   